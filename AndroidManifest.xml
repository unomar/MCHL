    	<activity android:name=".Matchup"></activity>
    	<activity android:name=".TeamStatsTab"></activity>
    	<activity android:name=".ComparisonTab"></activity>
	</application>
    <uses-sdk android:minSdkVersion="5" android:targetSdkVersion="5" />
    <uses-permission android:name="android.permission.INTERNET" /> 

</manifest> 
