<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.mchl.gui"
      android:versionCode="5"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:theme="@android:style/Theme.NoTitleBar">
        <activity android:name=".Omchl"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    	<activity android:name=".ConfigGUI"></activity>
    	<activity android:name=".Standings"></activity>
    	<activity android:name=".StandingsTab"></activity>
    	<activity android:name=".Schedule"></activity>
